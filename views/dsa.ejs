    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h2 class="text-center">Fill this form we will reach you out</h2>
          </div>
          <div class="modal-body">
            <form method="POST" action="/data">
              <label for="email" class="mt-2">Email</label>
              <input class="form-control" name="mail" placeholder="Enter your mail" id="mail" />
              <label for="name" class="mt-2">Name</label>
              <input class="form-control" name="name" placeholder="Enter your name" id="name" />
              <label for="time" class="mt-2">Contact no</label>
              <input class="form-control" name="phone" placeholder="Enter your contact no" id="phone" />
              <label for="time" class="mt-2">Course you are interested in</label>
              <select name="course" id="course" class="form-control">
                <option value="C and C++ foundation">C and C++ foundation</option>
                <option value="C++ intermediate + STL">C++ intermediate + STL</option>
                <option value="Intro to python">Intro to python</option>
                <option value="Intro to JAVA">Intro to JAVA</option>
                <option value="Frontend development">Frontend development</option>
                <option value="Augmented Reality">Augmented Reality</option>
              </select>
              <label for="source">From where you got to know about us</label>
              <select name="src" id="src" class="form-control">
                <option value="internet">internet</option>
                <option value="friend">friend</option>
                <option value="newspaper">newspaper</option>
                <option value="Exploration">Exploration</option>
              </select>
              <div class="text-center">
                <button class="btn btn-primary" id="updates" type="submit">Submit</button>
              </div>
            </form>
          </div>
        </div>
      </div>
